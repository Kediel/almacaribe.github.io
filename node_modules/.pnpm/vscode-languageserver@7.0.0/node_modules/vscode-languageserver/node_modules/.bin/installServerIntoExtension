#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/bin/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/bin/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules/vscode-languageserver/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/vscode-languageserver@7.0.0/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/installServerIntoExtension"   "$@"
exit $?
