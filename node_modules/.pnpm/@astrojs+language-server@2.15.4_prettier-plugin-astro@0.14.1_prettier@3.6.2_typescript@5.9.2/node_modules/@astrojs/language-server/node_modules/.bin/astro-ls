#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/language-server/bin/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/language-server/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/language-server/bin/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/language-server/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules/@astrojs/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/@astrojs+language-server@2.15.4_prettier-plugin-astro@0.14.1_prettier@3.6.2_typescript@5.9.2/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../../bin/nodeServer.js" "$@"
fi
