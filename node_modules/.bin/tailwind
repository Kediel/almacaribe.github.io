#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/Users/k3ma5/Desktop/AlmaCaribe/almacaribe.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/cli.js" "$@"
fi
